<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.autoinspectionapp.ui.home.HomeFragment"
        android:label="Home Fragment"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_navigation_home_to_carSchemanticFragment"
            app:destination="@id/carSchemanticFragment" />
    </fragment>


    <fragment
        android:id="@+id/carSchemanticFragment"
        android:name="com.example.autoinspectionapp.ui.carschemantics.CarSchemanticFragment"
        android:label="fragment_car_schemantic"
        tools:layout="@layout/fragment_car_schemantic" />


</navigation>