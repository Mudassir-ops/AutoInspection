<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.autoinspectionapp.ui.home.HomeFragment"
        android:label="Home Fragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_navigation_home_to_navigation_car_schemantic"
            app:destination="@id/navigation_car_schemantic" />
    </fragment>

    <fragment
        android:id="@+id/navigation_car_schemantic"
        android:name="com.example.autoinspectionapp.ui.home.pagerScreens.carDrawing.CarDrawingFragment"
        android:label="Car Drawing Fragment"
        tools:layout="@layout/fragment_car_drawing" />


</navigation>